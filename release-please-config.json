{
	"draft": true,
	"prerelease": false,
	"release-type": "node",
	"packages": {
		".": {
			"component": "app",
			"package-name": "Wombat",
			"changelog-path": "CHANGELOG.md",
			"exclude-paths": ["src-tauri"]
		},
		"src-tauri": {
			"component": "tauri-backend",
			"release-type": "rust",
			"extra-files": [
				{
					"type": "json",
					"path": "src-tauri/tauri.conf.json",
					"jsonpath": "$.version"
				}
			]
		}
	},
	"$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
	"bootstrap-sha": "223d5365a655b98258a86ef419e772b099f5ce70",
	"plugins": [
		"node-workspace",
		{
			"type": "linked-versions",
			"groupName": "wombat",
			"components": ["tauri-backend", "app"]
		}
	]
}
