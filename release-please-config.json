{
	"release-type": "node",
	"draft": true,
	"prerelease": false,
	"plugins": [
		{
			"type": "linked-versions",
			"groupName": "wombat",
			"components": ["app", "api"]
		}
	],
	"group-pull-request-title-pattern": "chore: release ${version}",
	"packages": {
		".": {
			"component": "app",
			"release-type": "node",
			"package-name": "wombat",
			"draft": true,
			"prerelease": false
		},
		"src-tauri": {
			"component": "api",
			"release-type": "rust",
			"skip-github-release": true,
			"extra-files": [
				{
					"type": "json",
					"path": "src-tauri/tauri.conf.json",
					"jsonpath": "$.version"
				}
			]
		}
	},
	"$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
	"bootstrap-sha": "223d5365a655b98258a86ef419e772b099f5ce70"
}
